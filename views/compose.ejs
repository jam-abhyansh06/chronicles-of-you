<%- include("partials/header") -%>

<h1>Compose</h1>


<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle" autocomplete="on">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="postContent" rows="8" cols="80" autocomplete="on"></textarea>
  </div>

  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials//footer") -%>
